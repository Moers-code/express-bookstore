{
	"$schema": "https://json-schema.org/draft/2019-09/schema",
	"$id": "http://example.com/example.json",
	"type": "object",
	"properties": {
		"isbn": {
			"type": "string",
			"examples": ["0691161518"]
		},
		"amazon_url": {
			"type": "string",
			"format": "uri",
			"examples": ["http://a.co/eobPtX2"]
		},
		"author": {
			"type": "string",
			"examples": ["Matthew Lane"]
		},
		"language": {
			"type": "string",
			"examples": ["english"]
		},
		"pages": {
			"type": "integer",
			"examples": [264]
		},
		"publisher": {
			"type": "string",
			"examples": ["Princeton University Press"]
		},
		"title": {
			"type": "string",
			"examples": ["Power-Up: Unlocking Hidden Math in Video Games"]
		},
		"year": {
			"type": "integer",
			"examples": [2017]
		}
	},
	"required": [
		"isbn",
		"amazon_url",
		"author",
		"language",
		"pages",
		"publisher",
		"title",
		"year"
	]
}
